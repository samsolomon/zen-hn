{
  "manifest_version": 3,
  "name": "Zen HN",
  "version": "0.1.0",
  "description": "Restyles Hacker News with custom CSS.",
  "content_scripts": [
    {
      "matches": ["https://news.ycombinator.com/*"],
      "css": ["styles.css"],
      "js": ["logic.js", "content.js"],
      "run_at": "document_start"
    }
  ]
}
